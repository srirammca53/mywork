<h1>Listing posts</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Dob</th>
    <th>Age</th>
    <th>Username</th>
    <th>Password</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @posts.each do |post| %>
  <tr>
    <td><%= post.Name %></td>
    <td><%= post.DOB %></td>
    <td><%= post.Age %></td>
    <td><%= post.Username %></td>
    <td><%= post.Password %></td>
    <td><%= link_to 'Show', post %></td>
    <td><%= link_to 'Edit', edit_post_path(post) %></td>
    <td><%= link_to 'Destroy', post, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Post', new_post_path %>
